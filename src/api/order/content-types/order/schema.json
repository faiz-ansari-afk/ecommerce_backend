{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "cancelled",
        "ordered",
        "processing",
        "shipped",
        "out for delivery",
        "completed"
      ],
      "required": true,
      "default": "ordered"
    },
    "order_id": {
      "type": "uid",
      "required": true
    },
    "final_price": {
      "type": "integer",
      "required": true
    },
    "expected_delivery_date": {
      "type": "date"
    },
    "expected_delivery_time": {
      "type": "time"
    },
    "payment": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ],
      "default": "unpaid",
      "required": true
    },
    "payment_mode": {
      "type": "enumeration",
      "enum": [
        "RAZORPAY",
        "COD"
      ],
      "required": true,
      "default": "COD"
    },
    "address": {
      "type": "json"
    },
    "payment_details": {
      "type": "enumeration",
      "enum": [
        "cash",
        "online"
      ]
    },
    "delivery_guy_details": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "notify": {
      "type": "boolean",
      "default": true,
      "required": true
    }
  }
}
